<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="world_name" default="$(find safer_stack)/worlds/sand_mine.world"/>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <!-- <arg name="world_name" value="$(arg world_name)"/> -->
    </include>

    <include file="$(find safer_stack)/launch/spawn/spawn_sdf.launch">
        <arg name="robot_name" value="sand_mine" />
        <arg name="x" value="0.0" />
        <arg name="y" value="0.0" />
        <arg name="z" value="0.0" />
        <arg name="roll" value="0.0"/>
        <arg name="pitch" value="0.0"/>
        <arg name="yaw" value="0.0" />
        <arg name="sdf_robot_file" value="$(find safer_stack)/models/sand_mine/model.sdf" />
    </include>
</launch>